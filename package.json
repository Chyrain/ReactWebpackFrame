{
  "name": "ReactWebpackFrame",
  "version": "0.0.1",
  "description": "A init frame for React project with webpack.",
  "main": "index.js",
  "repository": {
    "url": "https://github.com/Chyrain/ReactWebpackFrame.git",
    "type": "git"
  },
  "author": "Chyrain",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.dev.config.js --host 0.0.0.0 --port 7080 --progress --colors",
    "release": "NODE_ENV=production webpack --config ./webpack.dist.config.js --progress --colors",
    "build": "webpack --config ./webpack.dev.config.js --progress --colors",
    "build:test": "webpack --config ./webpack.test.config.js --progress --colors",
    "server:dev": "webpack-dev-server --config ./webpack.dev.config.js --port 7080 --progress --colors",
    "server:test": "webpack-dev-server --config ./webpack.test.config.js --port 8090 --progress --colors",
    "server:dist": "NODE_ENV=production webpack-dev-server --config ./webpack.dist.config.js --port 7090 --progress --colors"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "image-webpack-loader": "^3.0.0",
    "imagemin-gifsicle": "^5.1.0",
    "imagemin-mozjpeg": "^6.0.0",
    "imagemin-optipng": "^5.2.1",
    "imagemin-pngquant": "^5.0.0",
    "imagemin-svgo": "^5.2.0",
    "jasmine": "^2.5.2",
    "jasmine-react-helpers": "^0.2.2",
    "jasmine-webpack-plugin": "^0.1.1",
    "json-loader": "^0.5.4",
    "open-browser-webpack-plugin": "0.0.3",
    "postcss-loader": "^1.1.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.3.3",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-chunk-hash": "^0.2.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-md5-hash": "^0.0.5"
  },
  "dependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-transform-catch-errors": "^1.0.2"
  }
}
